{
  "redirects": [
    {
      "source": "/",
      "destination": "/research/research-catalog"
    }
  ],
  "functions": {
    "pages/search/index.tsx": {
      "maxDuration": 60
    },
    "pages/api/*.js": {
      "maxDuration": 60
    },
    "api/**/*.ts": {
      "includeFiles": "config/*.yaml"
    },
    "src/server/*": {
      "includeFiles": "config/*.yaml"
    }
  }
}
